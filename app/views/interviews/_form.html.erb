<%= form_with model: @interview,  local: true do |form| %>
        <p>
            <%= form.label:Start_time %>
            <%= form.time_field:start_t %>

        </p>
        <p>
            <%= form.label:End_time %>
            <%= form.time_field:end_t %>
        </p>

         <p>
            <%= form.label :Interviewee %><br>
             <%= form.collection_select( :intervieweeid , @participant1 , :id, :name) %>
            </p>
            <p>
            <%= form.label :Interviewer %><br>
             <%= form.collection_select( :interviewerid , @participant2 , :id, :name) %>
            </p>
            <p>
                <%= form.submit %>
            </p>
<% end %>
